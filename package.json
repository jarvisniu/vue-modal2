{
  "name": "vue-modal2",
  "version": "0.3.0",
  "description": "A vue modal solution",
  "main": "dist/vue-modal2.esm.js",
  "module": "dist/vue-modal2.esm.js",
  "scripts": {
    "dev": "parcel demo/index.html --out-dir demo/dist",
    "build-umd": "parcel build src/index.js --no-source-maps --out-dir dist --out-file vue-modal2.js",
    "build-esm": "rollup -c",
    "build": "rm -rf dist && mkdir dist && npm run build-esm"
  },
  "author": "Jarvis Niu",
  "license": "MIT",
  "homepage": "https://github.com/jarvisniu/vue-modal2#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jarvisniu/vue-modal2.git"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "autoprefixer": "^9.6.0",
    "parcel-bundler": "^1.12.3",
    "rollup": "^1.16.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.0.4",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-require-context": "^1.0.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-vue": "^5.0.0",
    "stylus": "^0.54.5",
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3",
    "vue-template-compiler": "^2.6.10"
  },
  "keywords": [
    "vue",
    "modal"
  ],
  "bugs": {
    "url": "https://github.com/jarvisniu/vue-modal2/issues"
  }
}
